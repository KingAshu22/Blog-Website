<%- include("partials/header"); -%>

    <h1>Home</h1>

    <p>
        <%= startingContent %>
    </p>

    <% posts.forEach(function(post) { %>
        <h1>
            <a href="/posts/<%= post.title %> ">
                <%= post.title %>
            </a>
        </h1>
        <p>
            <%= (post.content.substring(0, 250) + "..."); %>
                <a href="/posts/<%= post.title %> ">Read More</a>
        </p>
        <% }); %>
            <%- include("partials/footer"); -%>